{
    "datasets": [
        {
            "name": "titanic",
            "filename": "../data/titanic_train.csv"
        }
    ],
    "nodes": [
        {
            "name": "drop id y textos",
            "transformer": "ColumnDropper(columns=['PassengerId', 'Name', 'Ticket', 'Cabin', 'Embarked'])",
            "inputs": "['titanic']",
            "outputs": "['titanic_pro']"
        },
        {
            "name": "one hot encode sex",
            "transformer": "OneHotTransformer(column='Sex', encoder=OneHotEncoder, one_hot_columns=ndarray)",
            "inputs": "['titanic_pro']",
            "outputs": "['titanic_pro']"
        },
        {
            "name": "fillna numericos",
            "transformer": "DataframeTransformer(function=fillna_numerics)",
            "inputs": "['titanic_pro']",
            "outputs": "['titanic_pro']"
        },
        {
            "name": "split target",
            "transformer": "SplitFeaturesTarget(target='Survived')",
            "inputs": "['titanic_pro']",
            "outputs": "['titanic_X', 'titanic_y']"
        },
        {
            "name": "evaluacion",
            "transformer": "ClassificationModelEvaluator(name='rf_clf_eval', model=RandomForestClassifier, test_size=0.2, output_dir='../data/', _X_test=DataFrame, _y_test=DataFrame)",
            "inputs": "['titanic_X', 'titanic_y']",
            "outputs": "['titanic_X', 'titanic_y']"
        },
        {
            "name": "prediccion",
            "transformer": "SklearnModelTransformer(name='rf_clf', model=RandomForestClassifier)",
            "inputs": "['titanic_X', 'titanic_y']",
            "outputs": "['titanic_pred']"
        }
    ]
}